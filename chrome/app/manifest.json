{
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": []
  },
  "permissions": [
    "http://teams.microsoft.com/*",
    "https://teams.microsoft.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "http://teams.microsoft.com/*",
      "https://teams.microsoft.com/*"
    ],
    "js": [
      "scripts/refined.js"
    ],
    "css": [
      "styles/refined.css"
    ],
    "run_at": "document_end",
    "all_frames": false
  }]
}
